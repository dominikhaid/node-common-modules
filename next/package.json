  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "predev": "prettier --config .prettierrc.json \"{,!(node_modules)/**/}src/**/*.{js,jsx,ts,tsx}\" --write && eslint . --ext .ts --ext .tsx --ext .js --ext .jsx --fix && tsc --project ./tsconfig.server.json && ./convert.sh",
    "dev": "NODE_OPTIONS='--inspect' NODE_ENV=development node server.js",
    "prenext:build": "eslint . --ext .ts --ext .tsx --ext .js --ext .jsx --fix && ./convert.sh",
    "next:build": "next build",
    "prenext:dev": "eslint . --ext .ts --ext .tsx --ext .js --ext .jsx --fix && ./convert.sh",
    "next:dev": "next dev",
    "prenext:start": "eslint . --ext .ts --ext .tsx --ext .js --ext .jsx --fix && ./convert.sh",
    "next:start": "next start",
    "next:export": "next export",
    "lint": "eslint . --ext .ts --ext .tsx --ext .js --ext .jsx --fix",
    "format": "prettier --config .prettierrc.json \"{,!(node_modules)/**/}src/**/*.{js,jsx}\" --write",
    "next:debug": "node --inspect ./node_modules/next/dist/bin/next",
    "tailwind:build": "NODE_ENV=production postcss public/css/global.css -o public/css/tailwind-build.css",
    "tailwind:dev": "postcss public/css/global.css -o public/css/tailwind-build.css",
    "storybook:start": "start-storybook -c .storybook -s ./public -p 6006",
    "storybook:screenshot": "npx storycap http://localhost:6006",
    "storybook:build": "build-storybook && cp -R ./public/fonts ./storybook-static"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "globals": {
      "window": true,
      "browser": true
    }
